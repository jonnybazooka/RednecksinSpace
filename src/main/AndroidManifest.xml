<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.tgier.rednecksinspace">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".WorkerSetup">
        </activity>
        <activity android:name=".MinerSetup">
        </activity>
        <activity android:name=".BankerSetup">
        </activity>
        <activity android:name=".SlaveSetup">
        </activity>
        <activity android:name=".DifficultySetup">
        </activity>
        <activity android:name=".MainMenu">
        </activity>
        <activity
                android:name=".ShowWorkers"
                android:parentActivityName=".MainMenu">
        </activity>
        <activity
                android:name=".ShowResources"
                android:parentActivityName=".MainMenu">
        </activity>
        <activity
                android:name=".BuildingMenu"
                android:parentActivityName=".MainMenu">
        </activity>
    </application>

</manifest>